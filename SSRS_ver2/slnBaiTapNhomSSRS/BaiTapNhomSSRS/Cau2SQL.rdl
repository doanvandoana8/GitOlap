  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Cau2SQL</DataSourceName>
        <CommandText>SELECT        District.DISTRICT_NAME, COUNT(DISTINCT School.SCHOOL_ID) AS Expr1
FROM            School INNER JOIN
                         Fatc ON School.SCHOOL_ID = Fatc.SCHOOL_ID INNER JOIN
                         District ON Fatc.DISTRICT_NUMBER = District.DISTRICT_NUMBER INNER JOIN
                         SchoolYear ON Fatc.SCHOOL_YEAR_ID = SchoolYear.SCHOOL_YEAR_ID
GROUP BY District.DISTRICT_NAME, SchoolYear.START_YEAR
HAVING        (SchoolYear.START_YEAR = 2009)</CommandText>
      </Query>
      <Fields>
        <Field Name="DISTRICT_NAME">
          <DataField>DISTRICT_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr1">
          <DataField>Expr1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
