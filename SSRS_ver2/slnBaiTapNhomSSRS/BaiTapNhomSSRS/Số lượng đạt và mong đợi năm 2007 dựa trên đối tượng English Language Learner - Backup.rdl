    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="SCHOOL_NAME">
          <DataField>SCHOOL_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr1">
          <DataField>Expr1</DataField>
