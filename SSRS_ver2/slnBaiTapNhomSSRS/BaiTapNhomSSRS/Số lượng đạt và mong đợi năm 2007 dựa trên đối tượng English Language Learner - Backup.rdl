        <Field Name="Expr2">
          <DataField>Expr2</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Cau1FromSQL</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        School.SCHOOL_NAME, SUM(Fatc.NUMBER_WRITERS) AS Expr1, SUM(Fatc.NUMBER_EXPECTED_WRITERS) AS Expr2
FROM            Fatc INNER JOIN
                         School ON Fatc.SCHOOL_ID = School.SCHOOL_ID INNER JOIN
                         SchoolYear ON Fatc.SCHOOL_YEAR_ID = SchoolYear.SCHOOL_YEAR_ID INNER JOIN
                         SubPopulation ON Fatc.SUB_POPULATION_ID = SubPopulation.SUB_POPULATION_ID
GROUP BY School.SCHOOL_NAME, SchoolYear.START_YEAR, SubPopulation.SUB_POPULATION_NAME
HAVING        (SchoolYear.START_YEAR = 2007) AND (SubPopulation.SUB_POPULATION_NAME = 'ENGLISH LANGUAGE LEARNER')
ORDER BY School.SCHOOL_NAME</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
